/var/plog/upgrade.log
{
    daily
    missingok
    notifempty
    size +512k
    create 640 root root
    postrotate
       /usr/local/mindx/MindXOM/scripts/log_tar_save.sh /var/plog upgrade /home/log/plog 20
    endscript
}
/var/plog/ntp_service.log
{
    daily
    missingok
    notifempty
    size +512k
    create 640 root root
    postrotate
       /usr/local/mindx/MindXOM/scripts/log_tar_save.sh /var/plog ntp_service /home/log/plog 5
    endscript
}
/var/plog/web_edge/access.log
{
    daily
    dateext
    dateformat _%Y%m%d%s
    compress
    rotate 20
    copytruncate
    olddir /home/log/plog/web_edge
    missingok
    notifempty
    size +512k
    postrotate
       kill -USR1 $(/usr/bin/cat /usr/local/mindx/MindXOM/software/nginx/logs/nginx.pid) 2> /dev/null || true
    endscript
}
