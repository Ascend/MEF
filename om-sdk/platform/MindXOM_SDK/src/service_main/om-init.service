[Unit]
Description=MindX Edge OM init service depended by all other OM services
After=network-online.target devm_drv_init.service
Wants=network-online.target devm_drv_init.service

[Service]
Type=forking
ExecStart=/bin/bash -c '/usr/local/mindx/MindXOMUpgrade/scripts/om_init.sh || /usr/local/mindx/MindXOM/scripts/om_init.sh'
TimeoutSec=6000
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
